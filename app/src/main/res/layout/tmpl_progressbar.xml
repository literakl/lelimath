<?xml version="1.0" encoding="utf-8"?>
<com.github.lzyzsd.circleprogress.DonutProgress
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/progressBar"
    android:layout_width="40dp"
    android:layout_height="40dp"
    android:layout_marginEnd="10dp"
    android:layout_gravity="end"
    custom:donut_finished_color="@color/white"
    custom:donut_finished_stroke_width="4dp"
    custom:donut_unfinished_stroke_width="3dp"
    custom:donut_circle_starting_degree="270"
    custom:donut_text_size="10dp"
    custom:donut_text_color="@color/colorAccent"
    custom:donut_progress="25"
    android:layout_marginRight="10dp" />
